  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#8C959F",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#7A5AF8",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#2272CF"
      }
    },
    {
      "scope": ["variable", "meta.definition.variable", "support.variable"],
      "settings": {
        "foreground": "#1F2A30"
      }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#1F8A70"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language.boolean"],
      "settings": {
        "foreground": "#C47E1F"
      }
    },
    {
      "scope": ["constant", "variable.other.constant"],
      "settings": {
        "foreground": "#D6488B"
      }
    },
    {
      "scope": ["entity.name.type", "support.class", "support.type"],
      "settings": {
        "foreground": "#4F8DE2"
      }
    },
    {
      "scope": ["entity.name.tag", "support.type.property-name"],
      "settings": {
        "foreground": "#2272CF"
      }
    },
    {
      "scope": ["punctuation", "meta.brace", "meta.delimiter"],
      "settings": {
        "foreground": "#63707A"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#e742c9"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#2272CF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#1F2A30"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D6488B"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.raw"],
      "settings": {
        "foreground": "#1F8A70"
      }
    }
  ],
  "semanticTokenColors": {
    "property": "#2272CF",
    "function": "#2272CF",
    "method": "#2272CF",
    "macro": "#4F8DE2",
    "type": "#4F8DE2",
    "class": "#4F8DE2",
    "enum": "#7A5AF8",
    "parameter": "#1F2A30",
    "variable": "#1F2A30",
    "enumMember": "#C47E1F",
    "event": "#D6488B",
    "interface": "#4F8DE2",
    "keyword": "#7A5AF8",
    "comment": "#8C959F",
    "string": "#1F8A70",
    "number": "#C47E1F"
  }
}
